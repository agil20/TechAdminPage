    <input type="date" id="start-date" name="startTime" />

    <label for="end-date">Bitiş Tarihi:</label>
    <input type="date" id="end-date" name="endTime" />
           <table class="mytable">
  <tr>
    <th >Operatorun adı</th>
 
              
  </tr>

@foreach (var item in Model)
{
          <tr  >

